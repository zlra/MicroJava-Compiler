program test6
	int i, j, k;
{
	void t() int i; int j; int k; {
		i = 0;
		j = 0;
		k = 0;
		
        do {
        	i = i + 1;
        	print('i');
        	if (i == 2) break;
        	
        	do {
        		j = j + 1;
        		print('j');
        		if (j == 2) break;
        		
        		do {
        			k = k + 1;
        			print('k');
        			if (k == 2) break;
        		} while (1 != 0);
        	
        	} while (1 != 0);
        
        } while(1 != 0);
	}
	
	void main() {
		i = 50;
		j = 50;
		k = 50;
		t(); // i j k k j i
	}
}
